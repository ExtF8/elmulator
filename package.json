{
    "name": "elmulator",
    "version": "1.7.3",
    "description": "calculator for Elmar",
    "author": "ExtF8",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "electron-builder --linux --win"
    },
    "build": {
        "appId": "com.extfield8.elmulator",
        "productName": "Elmulator",
        "linux": {
            "icon": "./images/icons/32x32.png",
            "category": "Utility",
            "target": [
                "snap"
            ],
            "electronLanguages": [
                "en-US"
            ]
        },
        "snap": {
            "confinement": "strict",
            "summary": "Calculator for Elmar",
            "grade": "stable",
            "base": "core20"
        },
        "win": {
            "target": "portable",
            "electronLanguages": [
                "en-US"
            ],
            "icon": "./dist/.icon-ico/icon.ico",
            "asar": true,
            "compression": "maximum"
        },
        "copyright": "Copyright Â© 2024 ${author}"
    },
    "license": "MIT",
    "devDependencies": {
        "@electron-forge/cli": "^7.2.0",
        "@electron-forge/maker-deb": "^7.2.0",
        "@electron-forge/maker-rpm": "^7.2.0",
        "@electron-forge/maker-squirrel": "^7.2.0",
        "@electron-forge/maker-zip": "^7.2.0",
        "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
        "electron": "^30.0.3",
        "electron-builder": "^24.9.1",
        "electron-packager": "^17.1.2",
        "electron-packager-languages": "^0.5.0"
    },
    "dependencies": {
        "@electron/asar": "github:electron/asar",
        "@electron/packager": "github:electron/packager",
        "asar": "^3.2.0",
        "electron-squirrel-startup": "^1.0.0"
    }
}
